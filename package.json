{
  "name": "ngx-chartjs",
  "version": "0.0.0",
  "description": "Reactive, responsive, beautiful charts for Angular based on Chart.js. All options available there are also available",
  "scripts": {
    "code-style": "pretty-quick --staged",
    "precommit": "yarn code-style",
    "ng": "ng",
    "start": "ng serve demo",
    "build": "ng build --prod ngx-chartjs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:patch": "cd projects/ngx-chartjs && npm version patch && cd ../..",
    "lib:minor": "cd projects/ngx-chartjs && npm version minor && cd ../..",
    "lib:major": "cd projects/ngx-chartjs && npm version major && cd ../.."
  },
  "keywords": [
    "angular",
    "chart.js",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angularjs",
    "charts"
  ],
  "license": "MIT",
  "author": "Bo Vandersteene <bo@reibo.be>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bovandersteene/-reibo-ngx-chartjs.git"
  },
  "bugs": {
    "url": "https://github.com/bovandersteene/-reibo-ngx-chartjs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn code-style"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@types/chart.js": "^2.7.30",
    "chart.js": "^2.7.2",
    "chartjs-plugin-datalabels": "^0.4.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4"
  },
  "peerDependencies": {
    "chart.js": "^2.7.2"
  }
}
